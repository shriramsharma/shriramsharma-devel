[user]
	name = Shriram
	email = shriramsharma@ubuntusony.com
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local   = yellow
  remote  = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old  = red bold
  new  = green bold
[color "status"]
  added     = yellow
  changed   = green
  untracked = cyan
[core]
  excludesfile = "~/.gitignore"
  editor       = "vim"
[alias]
  co = commit
[push]
	default = tracking
