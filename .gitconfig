[user]
  name = Shriram Sharma
  email = ssharma5@walmartlabs.com
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local   = yellow
  remote  = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old  = red bold
  new  = green bold
[color "status"]
  added     = yellow bold
  changed   = red bold
  untracked = cyan bold
[core]
  excludesfile = "~/.gitignore"
  editor       = "vim"
[alias]
  co = commit
[push]
	default = tracking
